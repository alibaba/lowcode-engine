{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "strictNullChecks": true,
    "inlineSources": false,
    "lib": ["es6"],
    "downlevelIteration": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "./lib",
    "types": ["node"],
    "baseUrl": "."            /* Base directory to resolve non-absolute module names. */
  },
  "include": [
    "src/**/*"
  ]
}
