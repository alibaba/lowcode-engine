{
  "plugins": [
    [
      "build-plugin-component",
      {
        "filename": "editor-preset-vision",
        "library": "LowcodeEditor",
        "libraryTarget": "umd",
        "externals": {
          "react": "var window.React",
          "react-dom": "var window.ReactDOM",
          "prop-types": "var window.PropTypes"
        }
      }
    ],
    "build-plugin-fusion",
    ["build-plugin-moment-locales", {
      "locales": ["zh-cn"]
    }],
    "./build.plugin.js"
  ]
}
