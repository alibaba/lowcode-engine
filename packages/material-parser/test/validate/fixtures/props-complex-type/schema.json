{
    "componentName": "Select",
    "title": "下拉选择器",
    "description": "适用于下拉选择，下拉多选等场景",
    "tags": ["xx", "yy"],
    "docUrl": "https://fusion-demo.alibaba-inc.com/demos/next/select",
    "screenshot": "https://img.alicdn.com/tfs/TB1osvGNXzqK1RjSZFoXXbfcXXa-1149-854.png",
    "icon": "url/fsdfasdfa.svg",
    "devMode": "proCode",
    "npm": {
        "package": "@ali/deep",
        "exportName": "Button",
        "subName": "Icon.Option",
        "main": "",
        "destructuring": true,
        "version": "0.1.13"
    },
    "props": [
        {
            "name": "oneOf",
            "propType": {
                "type": "oneOf",
                "value": ["a", "b", "c"]
            }
        },
        {
            "name": "oneOfType",
            "propType": {
                "type": "oneOfType",
                "value": ["string", "number",  {
                    "type": "array",
                    "isRequired": true
                }]
            }
        },
        {
            "name": "arrayOf",
            "propType": "number"
        },
        {
            "name": "objectOf",
            "propType": "number"
        },
        {
            "name": "size",
            "propType": "number",
            "description": "尺寸",
            "defaultValue": 10
        },
        {
            "name": "dataSource",
            "propType": "array",
            "description": "下拉选项配置",
            "defaultValue": []
        },
        {
            "name": "shape",
            "propType": {
                "type": "shape",
                "value": [
                    {
                        "name": "color",
                        "propType": "string"
                    }, 
                    {
                        "name": "fontSize",
                        "propType": {
                            "type": "number",
                            "isRequired": true 
                        }  
                    }
                ]
            }
        },
        {
            "name": "exact",
            "propType": {
                "type": "exact",
                "value": [
                    {
                        "name": "name",
                        "propType": "string"
                    }, 
                    {
                        "name": "quantity",
                        "propType": {
                            "type": "number",
                            "isRequired": true 
                        }  
                    }
                ]
            }
        }
    ]
}