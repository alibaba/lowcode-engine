{
  "name": "@ali/lowcode-react-simulator-renderer",
  "version": "1.0.33-beta.0",
  "description": "react simulator renderer for alibaba lowcode designer",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "cloud-build": "build-scripts build --skip-demo",
