    "build:dts": "tsc -p tsconfig.declaration.json && node ../../scripts/rollup-dts.js",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
